data[i,1] = -data[i,1]
slacks <- function(data){
n <- nrow(data)
m <- ncol(data)
for (i in 1:n) {
if (data[i,4] < 0) {
data[i,1] = -data[i,1]
data[i,2] = -data[i,2]
data[i,4] = -data[i,4]
if(data[i,3]=='supeq'){
data[i,3]='infeq'
}
else{
data[i,3]='supeq'
}
}
}
V <- data.frame(data[1:n,1:(m-2)])
Z <- data.frame(data[,m])
I <- rbind(data.frame(diag(n-1)), 0)
I2<-cbind(data.frame(I[0]))
for (i in 1:n) {
if (data[i,3]=='supeq') {
I2<-cbind(I2,data.frame(I[i]))
I[i,] = -I[i,]
}
}
pos=nrow(I2)
for (i in 1:ncol(I2)) {
I2[pos,i]=-1
}
all <- data.frame(V,I,I2,Z)
return(all)
}
